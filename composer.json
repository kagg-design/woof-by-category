{
  "name": "kagg-design/woof-by-category",
  "description": "WooCommerce Product Filter (WOOF) extension to display set of filters depending on current product category page.",
  "license": "GPL-2.0",
  "keywords": [
    "woocommerce",
    "filter",
    "woocommerce product filter",
    "filter category"
  ],
  "homepage": "https://kagg.eu/en/",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "support": {
    "issues": "https://wordpress.org/support/plugin/woof-by-category"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/OnTheGoSystems/patchwork.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/PHPCompatibility/PHPCompatibilityWP.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "require": {
    "composer/installers": "^1.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "antecedent/patchwork": "^2.0",
    "sebastian/phpcpd": "^3.0",
    "10up/wp_mock": "~0.2",
    "phpunit/phpunit": "^5.7",
    "lucatume/function-mocker": "~1.3",
    "squizlabs/php_codesniffer": "^3.0",
    "phpcompatibility/php-compatibility": "^9.0",
    "phpcompatibility/phpcompatibility-wp": "^2.1.0",
    "wp-coding-standards/wpcs": "^2.0"
  },
  "autoload": {
    "classmap": [
      "classes"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "classes",
      "tests/phpunit/classes"
    ]
  }
}
