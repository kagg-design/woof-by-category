{
  "name": "kagg-design/woof-by-category",
  "description": "WooCommerce Product Filter (WOOF) extension to display set of filters depending on current product category page.",
  "keywords": [
    "woocommerce",
    "filter",
    "woocommerce product filter",
    "filter category"
  ],
  "homepage": "https://kagg.eu/en/",
  "type": "wordpress-plugin",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "KAGG Design",
      "homepage": "https://kagg.eu/en/",
      "email": "info@kagg.eu"
    }
  ],
  "support": {
    "issues": "https://wordpress.org/support/plugin/woof-by-category"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "composer/installers": "*",
    "woocommerce/woocommerce": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.6.*"
  },
"extra": {
    "installer-paths": {
      "../{$name}/": ["type:wordpress-plugin"]
    }
  }
}
